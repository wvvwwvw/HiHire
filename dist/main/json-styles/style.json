{
  "QtBinding": "PySide6",
  "ShowLogs": true,
  "LiveCompileQss": true,
  "CheckForMissingicons": false,
  "QMainWindow": [
    {
      "tittle": "HiHire",
      "frameless": true,
      "transluscentBg": true,
      "sizeGrip": "sizeGrip",
      "navigation": [
        {
          "minimize": "wrapBtn",
          "close": "closeBtn",
          "moveWindow": "header",
          "titleBar": "header",
          "restore": [
            {
              "buttonName": "changeWinBtn",
              "normalIcon": ":/feather/Icons/feather/square.png",
              "maximizedIcon": ":/feather/Icons/feather/copy.png"
            }
          ]
        }
      ]
    }
  ],
  "QSettings": [
    {
      "ThemeSettings": [
        {
          "CustomTheme": [
            {
              "Background-color": "#ffffff",
              "Text-color": "#010000",
              "Icons-color": "#000000",
              "Accent-color": "#addbf3",
              "Theme-name": "LightBlue",
              "Default-Theme": true
            },
            {
              "Background-color": "#1f2528",
              "Text-color": "#fefefe",
              "Icons-color": "#fefefe",
              "Accent-color": "#13aed3",
              "Theme-name": "DarkBlue",
              "Default-Theme": false
            }
          ]
        }
      ],
      "AppSettings": {
        "OrginizationName": "HiHire",
        "ApplicationName": "B3",
        "OrginizationDormain": "HiHire.org"
      }
    }
  ],
  "QPushButtonGroup": [
    {
      "Buttons": [
        "reportsBtn",
        "questionsBtn",
        "analysisBtn",
        "workersBtn"
      ],
      "Style": [
        {
          "Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-top: 1px solid $COLOR_BACKGROUND_3; border-left: 1px solid $COLOR_BACKGROUND_3;border-bottom: 1px solid $COLOR_BACKGROUND_3;}",
          "NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 2px solid transparent; border-left: 3px solid transparent;border-bottom: 3px solid transparent;}"
        }
      ]
    },
    {
      "Buttons": [
        "settingsBtn",
        "helpBtn",
        "infoBtn",
        "prifileBtn"
      ],
      "Style": [
        {
          "Active": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: bold; border-top: 2px solid $COLOR_BACKGROUND_3; border-left: 2px solid $COLOR_BACKGROUND_3;border-bottom: 2px solid $COLOR_BACKGROUND_3;}",
          "NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-top: 6px solid transparent; border-left: 6px solid transparent;border-bottom: 6px solid transparent;}"
        }
      ]
    },
    {
      "Buttons": [
        "closeLeftMenuBtn"
      ],
      "Style": [
        {
          "Active": "QPushButton{background-color: THEME.COLOR_BACKGROUND_2; font-weight: bold; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}",
          "NotActive": "QPushButton{background-color: transparent; font-weight: normal; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_1}"
        }
      ]
    },
    {
      "Buttons": [
        "addQuestionaryBtn",
        "exitFromQuestionBtn",
        "saveQuestionsBtn",
        "exitFromCandidateBtn",
        "acceptBtn",
        "agreeBtn",
        "disagreeBtn",
        "rejectBtn",
        "exportBtn",
        "importBtn",
        "esaveChangesBtn",
        "firedBtn",
        "vacationBtn",
        "workingBtn",
        "exitFromEmployeeBtn",
        "createUserBtn"
      ],
      "Style": [
        {
          "Active": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: bold; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_2}",
          "NotActive": "QPushButton{background-color: THEME.COLOR_ACCENT_1; font-weight: normal; border-radius: 5px} QPushButton::hover{background-color: THEME.COLOR_ACCENT_2}"
        }
      ]
    }
  ],
  "QCustomSlideMenu": [
    {
      "name": "leftMenu",
      "defaultSize": [
        {
          "width": 40,
          "height": 598
        }
      ],
      "collapsedSize": [
        {
          "width": 40,
          "height": "parent"
        }
      ],
      "expandedSize": [
        {
          "width": 185,
          "height": 598
        }
      ],
      "toggleButton": [
        {
          "buttonName": "menuBtn",
          "icons": [
            {
              "whenMenuIsCollapsed": ":/feather/icons/feather/align-justify.png",
              "whenMenuIsExpanded": ":/feather/icons/feather/arrow-left-circle.png"
            }
          ]
        }
      ],
      "menuTransitionAnimation": [
        {
          "animationDuration": 500,
          "animationEasingCurve": "OutCubic",
          "startWithAnimationDisabled": true
        }
      ]
    },
    {
      "name": "centerMenu",
      "defaultSize": [
        {
          "width": 0,
          "height": "parent"
        }
      ],
      "collapsedSize": [
        {
          "width": 0,
          "height": "parent"
        }
      ],
      "expandedSize": [
        {
          "width": 250,
          "height": "parent"
        }
      ],
      "menuTransitionAnimation": [
        {
          "animationDuration": 500,
          "animationEasingCurve": "Linear"
        }
      ]
    }
  ],
  "QCustomQStackedWidget": [
    {
      "name": "mainPages",
      "navigation": [
        {
          "navigationButtons": [
            {
              "reportsBtn": "reportsPage",
              "questionsBtn": "questionsPage",
              "analysisBtn": "dataAnalysPage",
              "workersBtn": "employeesPage"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "fade": [
            {
              "active": true,
              "duration": 500,
              "easingCurve": "InSine"
            }
          ]
        }
      ]
    },
    {
      "name": "centerMenuPages",
      "navigation": [
        {
          "navigationButtons": [
            {
              "settingsBtn": "settingsPage",
              "helpBtn": "helpPage",
              "infoBtn": "informationPage"
            }
          ]
        }
      ],
      "transitionAnimation": [
        {
          "fade": [
            {
              "active": true,
              "duration": 500,
              "easingCurve": "InSine"
            }
          ],
          "slide": [
            {
              "active": true,
              "duration": 500,
              "easingCurve": "InSine"
            }
          ]
        }
      ]
    }
  ]
}